    "ipfs-http-server": "^0.1.2",
    "multiformats": "^3.0.3",
    "os-utils": "^0.0.14",
    "datastore-s3": "^3.0.0",
    "aws-sdk": "^2.782.0",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/node": "^14.14.7",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.2",
